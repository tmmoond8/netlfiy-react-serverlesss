{
  "presets": [
    "@babel/typescript",
    "@babel/preset-react",
    "@babel/preset-env",
    ["@dr.pogodin/babel-preset-svgr", {
      "mimicCreateReactApp": true
    }]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "transform-html-import-to-string"
  ]
}
