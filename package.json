{
  "name": "netlify-serverless-deploy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "parse5": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.7.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev": "react-app-rewired start",
    "dev:lambda": "netlify-lambda serve lambda/functions",
    "babel:serverless": "yarn babel --config-file ./babel.serverless.json --out-dir lambda ./src --extensions \".js,.jsx,mjs,.ts,.tsx,.svg\"",
    "build": "react-app-rewired build",
    "build:serverless": "yarn babel:serverless && yarn tool:replace-svg-to-js && yarn tool:remove-env-d-js && yarn tool:cp-html",
    "build:netlify": "yarn build && yarn build:serverless",
    "tool:replace-svg-to-js": "yarn replace-in-files --string='.svg' --replacement='.js' lambda/icons/index.js",
    "tool:remove-env-d-js": "rm lambda/react-app-env.d.js",
    "tool:cp-html": "rm -rf lambda/asset && mkdir lambda/asset && cp build/server.html lambda/asset/"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@dr.pogodin/babel-preset-svgr": "^1.3.1",
    "@types/express": "^4.17.13",
    "@types/node-fetch": "^2.5.12",
    "@types/parse5": "^6.0.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "babel-plugin-transform-html-import-to-string": "^1.0.0",
    "customize-cra": "^1.0.0",
    "html-webpack-plugin": "^5.3.2",
    "netlify-lambda": "^2.0.14",
    "react-app-rewired": "^2.1.8",
    "replace-in-files-cli": "^1.0.0",
    "typescript": "^4.3.5"
  }
}
