{
  "name": "netlify-serverless-deploy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "nodemon dist-server/server.js",
    "dev:lambda": "netlify-lambda serve dist-server/functions",
    "build": "react-scripts build",
    "build:server": "yarn tsc -p tsconfig.server.json",
    "build:netlify": "yarn build && mv build/index.html build/server.html && yarn build:server",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "netlify-lambda": "^2.0.14",
    "typescript": "^4.3.5"
  }
}
